{

  "name": "Lorem Filler",
  "version": "1.0.0",

  "description": "Fill Lorem Ipsum into a page type \"Lorem;# \"",


  "page_action": {
    "default_icon": {
      "128": "icons/icon.png",
      "64": "icons/icon-64.png",
      "32": "icons/icon-32.png",
      "16": "icons/icon-16.png"
    }
  },

  "permissions": [
  "https://*/*",
  "http://*/*",
  "tabs"
],
  "content_scripts": [
    {
        "matches":["http://*/*","https://*/*"],
        "all_frames": true,
        "js": [
            "/js/onLoad.js"
        ]
    }
],

  "short_name": "Lorem Filler",

  "manifest_version": 2
}
